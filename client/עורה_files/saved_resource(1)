                            //]]>
                            $(document).ready(function() {
                                $('#previewForm').submit(function(e) {
                                    e.preventDefault();

                                    $.ajax({
                                        type: "POST",
                                        url: $('#previewForm').attr('action'),
                                        data: $('#previewForm').serialize(),
                                        success: function() {
                                            $('#previewForm').html("<div id='message' class='text-center'>Thank you! Please click the link we emailed you to complete your subscription.</div>");


                                        }
                                    });
                                    return false;
                                });
                            });
                        </script>
